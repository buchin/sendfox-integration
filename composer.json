{
    "name": "buchin/sendfox-integration",
    "description": "A Laravel package for integrating with the SendFox API.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Buchin",
            "email": "buchin@dropsugar.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Buchin\\SendFoxIntegration\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Buchin\\SendFoxIntegration\\SendFoxServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}